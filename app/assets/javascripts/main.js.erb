
Todo = angular.module("Todo", ['ngRoute']);

Todo.config(['$routeProvider', function($routeProvider){
  // Route for '/list'
  $routeProvider
    // .when('/list/new', { templateUrl: '../assets/mainCreatePost.html', controller: 'CreatePostCtrl' } )
    .when('/list/:listId', { templateUrl: "/listView.html'", controller: 'ListCtrl' } );

  //  Default
  $routeProvider.otherwise({ templateUrl: "/listIndex.html", controller: 'ListIndexCtrl' } );

}]);